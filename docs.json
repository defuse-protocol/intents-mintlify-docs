{
  "$schema": "https://mintlify.com/docs.json",
  "theme": "maple",
  "name": "NEAR Intents",
  "colors": {
    "primary": "#fb4d01",
    "light": "#ff7043",
    "dark": "#d94300"
  },
  "favicon": "/favicon.svg",
  "navigation": {
    "tabs": [
      {
        "tab": "Home",
        "icon": "house",
        "groups": [
          {
            "group": "Getting started",
            "pages": [
              "getting-started/landing",
              "getting-started/introduction",
              "getting-started/first-swap"
            ]
          },
          {
            "group": "Resources",
            "pages": [
              "resources/faqs",
              "resources/fees",
              {
                "group": "Security & Compliance",
                "icon": "shield-check",
                "pages": [
                  "security-compliance/risk-and-compliance",
                  "security-compliance/security",
                  "security-compliance/treasury-addresses",
                  "security-compliance/terms-of-service"
                ]
              }
            ]
          }
        ]
      },
      {
        "tab": "Token Swaps",
        "icon": "arrows-rotate",
        "groups": [
          {
            "group": "Token Swaps",
            "pages": [
              "integration/distribution-channels/introduction"
            ]
          },
          {
            "group": "1Click Swap API",
            "icon": "computer-mouse",
            "openapi": "/api-reference/openapi.json",
            "pages": [
              "integration/distribution-channels/1click-api/about-1click-api",
              "integration/distribution-channels/1click-api/authentication",
              "integration/distribution-channels/1click-api/fee-config",
              {
                "group": "API Endpoints",
                "icon": "code",
                "expanded": true,
                "pages": [
                  "GET /v0/tokens",
                  "POST /v0/quote",
                  "GET /v0/status",
                  "GET /v0/any-input/withdrawals",
                  "POST /v0/deposit/submit"
                ]
              }
            ]
          },
          {
            "group": "Explorer API",
            "icon": "magnifying-glass",
            "openapi": "/api-reference/explorer-openapi.yaml",
            "pages": [
              "api-reference/explorer/introduction",
              "GET /api/v0/transactions",
              "GET /api/v0/transactions-pages"
            ]
          }
        ]
      },
      {
        "tab": "Market Makers",
        "icon": "users",
        "groups": [
          {
            "group": "Market Makers",
            "pages": ["integration/market-makers/introduction"]
          },
          {
            "group": "Message Bus",
            "icon": "message",
            "pages": [
              "integration/market-makers/message-bus/introduction",
              "integration/market-makers/message-bus/api",
              "integration/market-makers/message-bus/usage-examples"
            ]
          },
          {
            "group": "Services",
            "pages": ["integration/market-makers/deposit-withdrawal-service"]
          },
          {
            "group": "Token Bridges",
            "icon": "bridge",
            "pages": ["integration/bridging/overview"]
          }
        ]
      },
      {
        "tab": "Verifier Contract",
        "icon": "shield-check",
        "groups": [
          {
            "group": "Verifier Contract",
            "pages": [
              "integration/verifier-contract/introduction",
              "integration/verifier-contract/account-abstraction"
            ]
          },
          {
            "group": "Deposits & Withdrawals",
            "icon": "arrow-right-arrow-left",
            "pages": [
              "integration/verifier-contract/deposits-and-withdrawals/overview",
              "integration/verifier-contract/deposits-and-withdrawals/balances",
              "integration/verifier-contract/deposits-and-withdrawals/deposits",
              "integration/verifier-contract/deposits-and-withdrawals/withdrawals",
              "integration/verifier-contract/deposits-and-withdrawals/near-token"
            ]
          },
          {
            "group": "Intents",
            "pages": [
              "integration/verifier-contract/intent-types-and-execution",
              "integration/verifier-contract/signing-intents",
              "integration/verifier-contract/simulating-intents",
              "integration/verifier-contract/events"
            ]
          }
        ]
      }
    ],
    "global": {
      "anchors": [
        {
          "anchor": "Supported Chains",
          "href": "/resources/chain-support",
          "icon": "check-circle"
        },
        {
          "anchor": "System Status",
          "href": "https://status.near-intents.org/posts/dashboard",
          "icon": "server"
        },
        {
          "anchor": "API Keys",
          "href": "/integration/distribution-channels/1click-api/authentication",
          "icon": "key"
        }
      ]
    }
