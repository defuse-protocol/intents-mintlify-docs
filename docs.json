  "logo": {
    "light": "/images/logo/light.svg",
    "dark": "/images/logo/dark.svg",
    "href": "https://near-intents.com"
  },
  "navbar": {
    "links": [
      {
        "label": "Explorer",
        "href": "https://explorer.near-intents.org/"
      },
      {
        "label": "Support",
        "href": "https://t.me/near_intents"
      }
    ],
    "primary": {
      "type": "button",
      "label": "Partner Dashboard",
      "href": "https://partners.near-intents.org/"
    }
  },
  "contextual": {
    "options": [
      "copy",
      "view",
      "chatgpt",
      "claude",
      "perplexity",
      "mcp",
      "cursor",
      "vscode"
    ]
  },
  "footer": {
    "socials": {
      "x": "https://x.com/near_intents",
      "github": "https://github.com/defuse-protocol"
    }
  },
  "appearance": {
    "default": "dark"
  }
}
